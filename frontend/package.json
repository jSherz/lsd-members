{
  "name": "luskydive",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0",
    "build": "ng build",
    "build-dev": "ng build --configuration=dev --progress=false --aot --output-hashing=all --vendor-chunk=false",
    "build-prod": "ng build --configuration=production --progress=false --aot --output-hashing=all --vendor-chunk=false --build-optimizer",
    "test": "ng test",
    "test-ci": "ng test --browsers ChromeHeadlessNoSandbox --watch=false --progress=false --code-coverage=true",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "e2e-ci": "ng e2e --config e2e/protractor-ci.conf.js --serve=false",
    "update-webdriver": "webdriver-manager update",
    "start-webdriver": "webdriver-manager start",
    "prettify": "prettier --write 'src/**/*.ts' 'e2e/**/*.ts'"
  },
  "engines": {
    "node": "~16",
    "npm": "~8.11"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "13.3.10",
    "@angular/compiler": "13.3.10",
    "@angular/core": "13.3.10",
    "@angular/forms": "13.3.10",
    "@angular/platform-browser": "13.3.10",
    "@angular/platform-browser-dynamic": "13.3.10",
    "@angular/router": "13.3.10",
    "core-js": "^2.6.12",
    "google-libphonenumber": "^3.2.27",
    "karma-coverage": "^2.2.0",
    "moment": "^2.29.3",
    "puppeteer": "^14.1.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.5",
    "tslib": "^2.4.0",
    "zone.js": "^0.11.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^13.3.7",
    "@angular-eslint/builder": "13.2.1",
    "@angular-eslint/eslint-plugin": "13.2.1",
    "@angular-eslint/eslint-plugin-template": "13.2.1",
    "@angular-eslint/schematics": "13.2.1",
    "@angular-eslint/template-parser": "13.2.1",
    "@angular/cli": "^13.3.7",
    "@angular/compiler-cli": "^13.3.10",
    "@angular/language-service": "13.3.10",
    "@types/google-libphonenumber": "^7.4.14",
    "@types/jasmine": "^4.0.3",
    "@types/jasminewd2": "^2.0.3",
    "@types/node": "^16.11.36",
    "@typescript-eslint/eslint-plugin": "5.17.0",
    "@typescript-eslint/parser": "5.17.0",
    "eslint": "^8.12.0",
    "jasmine-core": "^4.1.1",
    "jasmine-reporters": "^2.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "jsonwebtoken": "^8.5.1",
    "karma": "^6.3.20",
    "karma-chrome-launcher": "^3.1.1",
    "karma-cli": "^2.0.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "^5.0.1",
    "karma-jasmine-html-reporter": "^2.0.0",
    "mock-webstorage": "^1.0.3",
    "prettier": "^2.6.2",
    "protractor": "^7.0.0",
    "ts-node": "^10.8.0",
    "typescript": ">=4.4.2 <4.7"
  },
  "prettier": {
    "tabWidth": 2,
    "singleQuote": false
  }
}
